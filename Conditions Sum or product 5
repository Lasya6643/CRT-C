#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n1,n2;
    scanf("%d %d",&n1,&n2);
    int p=(n1*n2);
    if(p>1000){
    printf("%d",n1+n2);
    }
    else{
        printf("%d",p);
    }
    return 0;
}
