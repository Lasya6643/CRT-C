#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int s,l,b;
    float r,area;
    scanf("%d%d%d%f",&s,&l,&b,&r);
    area=3.14*r*r;
    printf("%d\n%d\n%.2f",s*s,l*b,area);
    
    return 0;
}
