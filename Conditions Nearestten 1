#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    scanf("%d",&n);
    int rem=n%10;
    if(rem<5){
        printf("%d",n-rem);
    }
    else{
        printf("%d",(n+10)-rem);
    }
    
    return 0;
}
